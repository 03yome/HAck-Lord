writefile("?.mp3", game:HttpGet("https://github.com/03yome/HAck-Lord/blob/main/y2mate--ANNIHILATION-John-Doe-vs-FRIEND-Elliot-CLOSE-TO-ME-Red.mp3"))

-- Tạo sound instance
local sound = Instance.new("Sound")
sound.SoundId = getcustomasset("?.mp3")
sound.Name = "?"
sound.Volume = 5
sound.Looped = false
sound.Parent = workspace
 
-- Phát nhạc
sound:Play()
